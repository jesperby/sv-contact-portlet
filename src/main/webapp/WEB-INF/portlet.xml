<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns:sv="http://www.senselogic.se/2006/11/07/portlet" xmlns="http://java.sun.com/xml/ns/portlet"
             version="1.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:noNamespaceSchemaLocation="http://java.sun.com/xml/portlet.xsd">     	
	<portlet>
		<sv:use-config-window>true</sv:use-config-window>
		<portlet-name>kontaktruta-stadsomraden</portlet-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
        	<name>contextConfigLocation</name>
        	<value>/WEB-INF/context/kontaktruta-stadsomraden-portlet.xml</value>
      	</init-param>
      	<expiration-cache>0</expiration-cache>      	
      	<supports>
        	<mime-type>text/html</mime-type>
        	<portlet-mode>view</portlet-mode>
         	<portlet-mode>config</portlet-mode>
      	</supports>
      	<portlet-info>
        	<title>Kontaktruta Stadsomr√•den</title>
      	</portlet-info>
		<portlet-preferences>
			<preference>
				<name>districtSelector</name>
				<value>false</value>
				<read-only>true</read-only>
			</preference>		
			<preference>
				<name>text</name>
				<value></value>
				<read-only>true</read-only>
			</preference>
      	</portlet-preferences>
      	<sv:window>
      		<sv:portlet-mode>config</sv:portlet-mode>
      		<sv:width>480</sv:width>
      		<sv:height>685</sv:height>
      	</sv:window>
	</portlet>	
     <custom-portlet-mode>
		<portlet-mode>config</portlet-mode>
	</custom-portlet-mode>
</portlet-app>